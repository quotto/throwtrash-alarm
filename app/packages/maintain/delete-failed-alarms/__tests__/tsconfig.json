    "compilerOptions": {
        "rootDir": ".",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "extends": "../tsconfig.json",
    "include": ["**/*"]
}